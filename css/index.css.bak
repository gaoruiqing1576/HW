*{margin:0;padding:0}
div.container{
	padding:0px;
}

a:hover{
	text-decoration: none;
	color:#777;
}
a{
	text-decoration: none;
	color:#777;
	font-size:14px;
}
.my_header,.my_footer{
    width:100%;
}
@media (min-width: 1200px) {
  .container {
    max-width: 1200px;
  }
}
/*大屏幕下的iframe的高*/
@media (min-width:992px){
    .my_header{  height: 7rem;  }
    .my_footer{  height: 38rem;  }
}
/*中小屏幕下的iframe的高*/
@media (max-width:992px) and (min-width:576px){
    .my_header{  height: 10rem;  }
    .my_footer{  height: 30rem;  }
}

div.container{
	padding:0px;
}
.carousel{
	width:1200px;
	margin:0 auto;
}
.carousel-inner{
	border-radius:20px;
}
.carousel-control-prev,
.carousel-control-next {
	width:4%; height:15%;
	background-color:rgb(40,40,28);
	top:40%;
	border-radius:50%;
}
div.container #demo2{
	height:120px;
}

.carousel-indicators li {
	width:0.8rem;height: 0.8rem;
	background-color: #fff;
	border-radius: 50%;
	margin:0 5px;
}

.carousel-indicators .active {
	background-color: #0aa1ed;
}
/*导航*/
/*左*/
.menu{
	width:1200px;
	height:110px ;
	border:1px solid #dddddd;
	border-top-left-radius:15px ;
	border-top-right-radius:15px ;

}
.menu ul li img{
	width:52px ;
	height:52px ;
}
.menu ul li .left{
	width:270px ;

	padding-left:26px;
}
.menu ul li .left div{
	padding:10px;
	font-size: 14px;
}
.menu ul li .left div p:first-child a{
	color:#333;
	font-size: 14px;
}
.menu ul li .left div p:last-child a:first-child{
	color:#DE5B60;
	font-size: 13px;
	background:#fff ;
	border: 1px solid #f00;
	padding: 3px;
	border-radius: 4px;
	margin-right: 10px;
}
.menu ul li .left div p:last-child a:last-child{
	color:#FDE4B3;
	font-size: 13px;
	background:#595454 ;
	padding: 3px;
	border-radius: 4px;
}
/*中*/
.font_size{
	color:#777;
	font-size: 12px;
}
.center{
	width:590px;
	height:100px;
	list-style:none;
	margin:0px;
}
.center li{
	float:left;
	text-align:center;
}
.menu ul li .center li div{
	width:76px;
	height:76px;
	background-image:url(https://res.vmallres.com/pimages//squaredInfo/icon/5/c70fc37c27f420183298024a56189a84.png);
	transform:scale(0.5);
}
.pic_01{
	background-position:0px 0px;
}
.pic_02{
	background-position:-80px 0px;
}
.pic_03{
	background-position:-160px 0px;
}
.pic_04{
	background-position:-240px 0px;
}
.pic_05{
	background-position:-320px 0px;
}
.pic_06{
	background-position:200px -2px;
}
/*右*/

.menu ul li .right div{
	width:35px;
	height:35px;
	background-image:url(https://res.vmallres.com/pimages//squaredInfo/icon/5/c70fc37c27f420183298024a56189a84.png);
	transform:scale(0.5);
}
.menu ul li .right li{
	widtg:33%;
	list-style: none;
}
.menu ul li .right p{
	color:#777;
	font-size:14px;
	line-height:36px;
}

.pic_07{
	background-position:120px 0px;
}
.pic_08{
	background-position:80px 0px;
}
.pic_09{
	background-position:40px -2px;
}


/*F4*/
.F4 ul>li+li{
	margin-left:13px;
}
.F4 img{
	border-radius: 20px;
}
.F4{
	margin: 15px 0px 15px 0px;
}



.floor-1 .top,.floor-2 .top,.floor-3 .top{
	width:1200px;height:32px;
	margin:15px auto;
}

.recommend h2,.floor-1 .top h2,.floor-2 .top h2,.floor-3 .top h2{
	width:130px;height:32px;
	font-size:26px;
	color:#333;
	margin:0;
	float:left;
	margin-right: 24px;
}
.floor-1 .top h2{
	width:52px;height:32px;
}
.recommend h2{
	width:100%;
	height:47px;
}

.floor-1 .top ul,.floor-2 .top ul,.floor-3 .top ul{
	width:268px;height:32px;
	float:left;
	list-style: none;
}
.floor-1 .top ul{
	width:1000px;
}

.floor-1 .top ul li,.floor-2 .top ul li,.floor-3 .top ul li{
	float:left;
	line-height: 32px;
}
.floor-1 .top ul li a,.floor-2 .top ul li a,.floor-3 .top ul li a{
	font-size:14px;
	color:#999;
	margin-right: 24px;
}

.floor-1 .top div,.floor-2 .top div,.floor-3 .top div{
	float:right;
}
.floor-1 .top div a,.floor-2 .top div a,.floor-3 .top div a{
	font-size:14px;
	color:#333;
	line-height: 32px;
}

.recommend ul,.floor-1 .bottom,.floor-2 .bottom,.floor-3 .bottom{
	width:1210px;
	float:left;
}

.fl_img{
	width:230px;
	height:290px;
}

.recommend ul,.floor-1 .bottom ul,.floor-2 .bottom ul,.floor-3 .bottom ul{
	list-style: none;
}

.recommend ul li,.floor-1 .bottom ul li,.floor-2 .bottom ul li,.floor-3 .bottom ul li{
	width:230px;height:290px;
	text-align: center;
	float:left;
	margin-left: 12px;
	margin-bottom: 12px;
}
.recommend ul li img,.floor-1 .bottom ul li img,.floor-2 .bottom ul li img,.floor-3 .bottom ul li img{
	width:150px;height:150px;
	margin-top:48px;
}
.recommend ul li p:nth-child(2),.floor-1 .bottom ul li p:nth-child(2),.floor-2 .bottom ul li p:nth-child(2),.floor-3 .bottom ul li p:nth-child(2){
	font-size: 14px;
	color:#3A3A3A;
	margin:12px 10px 0px;
}
.recommend ul li p:nth-child(3),.floor-1 .bottom ul li p:nth-child(3),.floor-2 .bottom ul li p:nth-child(3),.floor-3 .bottom ul li p:nth-child(3){
	font-size: 12px;
	color:#777;
	margin:2px 20px 7px;
	padding:0 10px;
}
.recommend ul li p:nth-child(4),.floor-1 .bottom ul li p:nth-child(4),.floor-2 .bottom ul li p:nth-child(4),.floor-3 .bottom ul li p:nth-child(4){
	font-size: 14px;
	color:#D00218;
	margin: 0;
}














